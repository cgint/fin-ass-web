{
  "name": "fin-ass-web",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "npm run build && wrangler pages dev dist --live-reload",
    "deploy": "npm run build && wrangler pages deploy dist",
    "watch": "npm-watch",
    "svelte-check": "svelte-check"
  },
  "devDependencies": {        
    "svelte": "4.*",
    "vite": "5.*",
    "axios": "0.*",
    "@sveltejs/vite-plugin-svelte": "3.*",
    "rollup": "4.*",
    "wrangler": "3.*",
    "bootstrap": "5.*",
    "npm-watch": "0.*",
    "svelte-check": "3.*"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "index.html"
      ],
      "extensions": "*"
    }
  }
}
