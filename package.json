{
  "name": "fin-ass-web",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "npm run build && wrangler pages dev dist --live-reload",
    "deploy": "npm run build && wrangler pages deploy dist",
    "watch": "npm-watch",
    "svelte-check": "svelte-check"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "5.*",
    "svelte": "5.*",
		"svelte-check": "4.*",
    "vite": "6.*",
		"vitest": "2.*",
		"wrangler": "3.*",
    "axios": "1.*",
    "bootstrap": "5.*",
    "npm-watch": "*"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "index.html"
      ],
      "extensions": "*"
    }
  }
}
